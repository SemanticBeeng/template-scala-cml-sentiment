{
  "id": "default",
  "description": "Default settings",
  "engineFactory": "sentiment.SentimentEngine",
  "sparkConf": {
      "spark": {
        "executor": {
          "memory": "12g"
        },
        "storage.memoryFraction": 0.1,
        "driver.memory": "12g"
      }
  },
  "datasource": {
    "params" : {
      "fraction": 0.01,
      "batchSize": 30
    }
  },
  "algorithms": [
    {
      "name": "rntn",
      "params": {
        "wordVecSize": 5,
        "stepSize": 0.1,
        "regularizationCoeff": 1e-6,
        "iterations": 10,
        "noise": 0.1
      }
    }
  ]
}
