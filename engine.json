{
  "id": "default",
  "description": "Default settings",
  "engineFactory": "sentiment.SentimentEngine",
  "sparkConf": {
      "spark": {
        "executor": {
          "memory": "12g"
        },
        "storage.memoryFraction": 0.05,
        "driver.memory": "12g"
      }
  },
  "datasource": {
    "params" : {
      "fraction": 1,
      "batchSize": 25
    }
  },
  "algorithms": [
    {
      "name": "rntnSplit",
      "params": {
        "halfVecSize": 5,
        "stepSize": 0.1,
        "regularizationCoeff": 1e-6,
        "iterations": 500,
        "noise": 0.1
      }
    }
  ]
}
